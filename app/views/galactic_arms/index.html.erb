<p id="notice"><%= notice %></p>

<h1>Galactic arms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @galactic_arms.find_each do |galactic_arm| %>
      <tr>
        <td><%= link_to 'Show', galactic_arm %></td>
        <td><%= link_to 'Edit', edit_galactic_arm_path(galactic_arm) %></td>
        <td><%= link_to 'Destroy', galactic_arm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Galactic arm', new_galactic_arm_path %>
